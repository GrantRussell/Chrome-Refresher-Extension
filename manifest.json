{
    "name": "Chrome Refresher",
    "version": "1.1",
    "description": "Chrome Refesher on interval",
    "manifest_version": 2,
    "permissions": ["declarativeContent", "tabs", "storage", "alarms"],
    "background": {
        "scripts": ["refresher.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "images/icon32.png",
        "default_popup": "popup.html"
    }
}