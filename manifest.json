{
    "name": "Chrome Refresher",
    "version": "1.0",
    "description": "Chrome Refesher on interval",
    "manifest_version": 2,
    "permissions": ["declarativeContent", "tabs"],
    "background": {
        "scripts": ["refresher.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    }
}